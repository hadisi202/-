# 数据库配置
DB_PATH=packing_system.db
DB_TIMEOUT=10.0

# API服务器配置
API_HOST=127.0.0.1
API_PORT=5000
API_DEBUG=false

# API认证配置
API_KEY_REQUIRED=true
# 多个密钥用逗号分隔
API_KEYS=your_secret_key_1,your_secret_key_2

# CORS配置
CORS_ENABLED=true
# 允许的来源，多个用逗号分隔，支持通配符
CORS_ORIGINS=http://localhost:*,http://127.0.0.1:*

# 日志配置
LOG_DIR=logs
LOG_LEVEL=INFO

# 系统目录配置
BACKUP_DIR=backups
ORDERS_DIR=orders
TEMPLATES_DIR=templates
